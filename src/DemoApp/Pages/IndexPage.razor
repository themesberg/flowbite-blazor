    window.flowbiteSetLightMode = function () {
        let isDark = localStorage.getItem('color-scheme') === 'dark'
        if (isDark) {
            window.flowbiteToggleDarkMode()
        }
    }

    window.flowbiteSetDarkMode = function () {
        let isDark = localStorage.getItem('color-scheme') === 'dark'
        if (!isDark) {
            window.flowbiteToggleDarkMode()
        }
    }


